{
    "cSpell.words": [
        "Firstname",
        "gsettings",
        "ruamel",
        "sysconf",
        "unsets"
    ],
    "cSpell.enabledFileTypes": {
        "yaml": false,
        "gitignore": false
    },
    "cSpell.ignorePaths": [
        "package-lock.json",
        "node_modules",
        "vscode-extension",
        ".git/{info,lfs,logs,refs,objects}/**",
        ".git/{index,*refs,*HEAD}",
        ".vscode",
        ".vscode-insiders",
        ".gitignore"
    ],
    "editor.rulers": [
        80
    ],
    "python.analysis.diagnosticMode": "workspace",
}